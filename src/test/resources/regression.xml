<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE suite SYSTEM "http://testng.org/testng-1.0.dtd">
<suite thread-count="2" name="Regression" parallel="classes">
    <groups>
        <run>
            <include name="Regression"/>
        </run>
    </groups>
    <parameter name="browser" value="chrome"/>
    <test name="Calculators tests">
        <classes>
            <class name="tests.OtherCalculatorsTest"/>
            <class name="tests.WorkoutCalculatorsTest"/>
        </classes>
    </test> <!-- Test -->
    <test name="Logout, DailyVitals, PrintWorkoutsTest tests">
        <classes>
            <class name="tests.LogoutTest"/>
            <class name="tests.DailyVitalsTest"/>
            <class name="tests.PrintWorkoutsTest"/>
        </classes>
    </test> <!-- Test -->
</suite>